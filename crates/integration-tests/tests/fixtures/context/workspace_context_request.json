{
  "name": "Context Request with Pre-made Workspace",
  "description": "Tests context request tool using pre-made rust-project workspace",
  "tags": [
    "context",
    "workspace",
    "embeddings"
  ],
  "setup": {
    "workspace": "rust-project",
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Show me the User struct",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "User struct",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const result = await requestContext(",
          "    'src/main.rs',",
          "    'Need User struct implementation',",
          "    5",
          "  );",
          "  return 'Found User struct in main.rs';",
          "}"
        ]
      },
      "verify": {
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {}
  }
}
