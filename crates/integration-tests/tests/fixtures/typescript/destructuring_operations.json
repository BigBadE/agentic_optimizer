{
  "name": "TypeScript Destructuring Assignment",
  "description": "Tests object and array destructuring patterns",
  "tags": [
    "typescript",
    "destructuring",
    "syntax"
  ],
  "setup": {
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Test destructuring patterns",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "destructuring",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const user = { name: 'Alice', age: 30, city: 'NYC' };",
          "  const { name, age } = user;",
          "  const nums = [1, 2, 3, 4, 5];",
          "  const [first, second, ...rest] = nums;",
          "  return `Name: ${name}, Age: ${age}, First: ${first}, Rest: ${rest.length}`;",
          "}"
        ]
      },
      "verify": {
        "ui": {
          "tasks_displayed": 1,
          "output_contains": [
            "Name: Alice",
            "Age: 30",
            "First: 1",
            "Rest: 3"
          ]
        },
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {
      "all_tasks_completed": true
    }
  }
}
