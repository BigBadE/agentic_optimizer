{
  "name": "Keyboard Navigation",
  "description": "Tests keyboard navigation in TUI (arrow keys, tab, etc.)",
  "tags": [
    "tui",
    "navigation",
    "keyboard"
  ],
  "setup": {
    "files": {},
    "terminal_size": [
      120,
      40
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "First task",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "verify": {
        "execution": {}
      },
      "strategy": {
        "type": "once",
        "trigger": {
          "pattern": "First task",
          "match_type": "contains"
        },
        "response": {
          "typescript": [
            "async function agent_code(): Promise<string> {",
            "  return 'Task 1 completed';",
            "}"
          ]
        }
      }
    },
    {
      "type": "key_press",
      "data": {
        "key": "Tab"
      }
    },
    {
      "type": "key_press",
      "data": {
        "key": "Down"
      }
    },
    {
      "type": "key_press",
      "data": {
        "key": "Up"
      }
    },
    {
      "type": "key_press",
      "data": {
        "key": "Tab"
      }
    },
    {
      "type": "user_input",
      "data": {
        "text": "Second task after navigation",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "verify": {
        "execution": {}
      },
      "strategy": {
        "type": "once",
        "trigger": {
          "pattern": "Second task",
          "match_type": "contains"
        },
        "response": {
          "typescript": [
            "async function agent_code(): Promise<string> {",
            "  return 'Task 2 completed';",
            "}"
          ]
        }
      }
    }
  ],
  "final_verify": {
    "execution": {}
  }
}
