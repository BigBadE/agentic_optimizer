{
  "name": "Vector Cache Completed",
  "description": "Tests UI after embedding cache completes",
  "config": {
    "terminal_size": [100, 30],
    "enable_vector_cache": true,
    "mock_embedding_speed": 10
  },
  "initial_state": {
    "workspace_files": [
      {"path": "src/main.rs", "content": "fn main() { println!(\"Hello\"); }"},
      {"path": "src/lib.rs", "content": "pub fn test() {}"}
    ],
    "vector_cache_state": "empty"
  },
  "steps": [
    {
      "step": 1,
      "action": {
        "type": "user_input",
        "data": {
          "text": "Analyze the codebase",
          "submit": true
        }
      },
      "expectations": {}
    },
    {
      "step": 2,
      "action": {
        "type": "agent_response",
        "data": {
          "text": "I'll analyze the codebase for you.",
          "tool_calls": [],
          "subtasks": []
        }
      },
      "expectations": {}
    },
    {
      "step": 3,
      "action": {
        "type": "background_event",
        "data": {
          "event_type": "embedding_progress",
          "progress": 2,
          "total": 2
        }
      },
      "expectations": {}
    },
    {
      "step": 4,
      "action": {
        "type": "background_event",
        "data": {
          "event_type": "embedding_complete",
          "progress": 2,
          "total": 2
        }
      },
      "expectations": {
        "ui_state": {
          "snapshot": "vector_cache_completed.txt",
          "has_progress": false
        }
      }
    }
  ]
}
