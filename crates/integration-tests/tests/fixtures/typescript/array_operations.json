{
  "name": "TypeScript Array Operations",
  "description": "Tests array manipulation and operations in TypeScript runtime",
  "tags": [
    "typescript",
    "arrays",
    "data-structures"
  ],
  "setup": {
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Process array of numbers",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "array",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const numbers = [1, 2, 3, 4, 5];",
          "  const doubled = numbers.map(n => n * 2);",
          "  const sum = doubled.reduce((a, b) => a + b, 0);",
          "  return `Sum: ${sum}`;",
          "}"
        ]
      },
      "verify": {
        "ui": {
          "tasks_displayed": 1,
          "output_contains": [
            "Sum: 30"
          ]
        },
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {},
    "ui": {
      "all_tasks_completed": true,
      "tasks_displayed": 1
    }
  }
}
