{
  "name": "File Read and Write Operations",
  "description": "Tests file reading, modification, and writing operations",
  "initial_query": "Read the config file and update the version to 2.0",
  "mock_responses": [
    {
      "pattern": "Read the config file",
      "response": [
        "```typescript",
        "async function agent_code(): Promise<string> {",
        "  const content = await readFile('config.txt');",
        "  const updated = content.replace('version=1.0', 'version=2.0');",
        "  await writeFile('config.txt', updated);",
        "  return \"Updated config version to 2.0\";",
        "}",
        "```"
      ],
      "expected_tool_calls": [
        {
          "tool": "readFile",
          "args_pattern": "config.txt",
          "result_pattern": null
        },
        {
          "tool": "writeFile",
          "args_pattern": "config.txt",
          "result_pattern": null
        }
      ],
      "use_once": false,
      "should_fail": false
    }
  ],
  "expected_task_list": null,
  "expected_outcomes": {
    "all_tasks_completed": true,
    "files": [
      {
        "path": "config.txt",
        "contains": [
          "version=2.0"
        ],
        "not_contains": [
          "version=1.0"
        ],
        "exact_content": null,
        "must_exist": true,
        "must_not_exist": false
      }
    ],
    "validation_passed": true,
    "response": {
      "contains": [
        "config",
        "2.0"
      ],
      "not_contains": [],
      "min_length": 10
    },
    "min_tool_calls": 2,
    "max_tool_calls": 5,
    "min_provider_calls": 1,
    "max_provider_calls": 3
  },
  "setup_files": {
    "config.txt": "app_name=MyApp\nversion=1.0\nauthor=Test"
  },
  "env_vars": {},
  "tags": ["file_ops", "read_write"]
}
