{
  "name": "TypeScript Try-Catch Error Handling",
  "description": "Tests try-catch blocks and error handling in TypeScript runtime",
  "tags": [
    "typescript",
    "error-handling",
    "exceptions"
  ],
  "setup": {
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Handle errors gracefully",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "verify": {
        "ui": {
          "tasks_displayed": 1,
          "output_contains": [
            "Caught error: Test error"
          ]
        },
        "execution": {}
      },
      "strategy": {
        "type": "once",
        "response": {
          "typescript": [
            "async function agent_code(): Promise<string> {",
            "  try {",
            "    throw new Error('Test error');",
            "  } catch (e) {",
            "    return `Caught error: ${e.message}`;",
            "  }",
            "}"
          ]
        }
      }
    }
  ],
  "final_verify": {
    "execution": {}
  }
}