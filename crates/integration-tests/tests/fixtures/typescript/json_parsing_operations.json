{
  "name": "TypeScript JSON Parsing and Stringification",
  "description": "Tests JSON.parse and JSON.stringify operations",
  "tags": [
    "typescript",
    "json",
    "parsing"
  ],
  "setup": {
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Test JSON operations",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "JSON",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const obj = { name: 'Test', value: 42, nested: { key: 'value' } };",
          "  const jsonStr = JSON.stringify(obj);",
          "  const parsed = JSON.parse(jsonStr);",
          "  return `Name: ${parsed.name}, Value: ${parsed.value}, Nested: ${parsed.nested.key}`;",
          "}"
        ]
      },
      "verify": {
        "ui": {
          "tasks_displayed": 1,
          "output_contains": [
            "Name: Test",
            "Value: 42",
            "Nested: value"
          ]
        },
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {}
  }
}
