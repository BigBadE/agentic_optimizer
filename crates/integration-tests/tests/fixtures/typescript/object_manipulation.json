{
  "name": "TypeScript Object Manipulation",
  "description": "Tests object creation, modification, and property access",
  "tags": [
    "typescript",
    "objects",
    "data-structures"
  ],
  "setup": {
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Process object data",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "object",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const user = {",
          "    name: 'Alice',",
          "    age: 30,",
          "    email: 'alice@example.com'",
          "  };",
          "  user.age += 1;",
          "  const keys = Object.keys(user);",
          "  return `User: ${user.name}, Age: ${user.age}, Keys: ${keys.length}`;",
          "}"
        ]
      },
      "verify": {
        "ui": {
          "tasks_displayed": 1,
          "output_contains": [
            "Alice",
            "Age: 31",
            "Keys: 3"
          ]
        },
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {}
  }
}
