{
  "name": "TUI Embedding Progress",
  "description": "Tests background progress indicators",
  "setup": {
    "workspace_files": [],
    "env_vars": {},
    "terminal_size": [80, 24]
  },
  "steps": [
    {
      "step": 1,
      "action": {
        "type": "send_ui_event",
        "data": {
          "type": "task_started",
          "description": "Embedding task"
        }
      },
      "expectations": {}
    },
    {
      "step": 2,
      "action": {
        "type": "send_ui_event",
        "data": {
          "type": "task_progress",
          "progress": 0
        }
      },
      "expectations": {}
    },
    {
      "step": 3,
      "action": {
        "type": "send_ui_event",
        "data": {
          "type": "task_progress",
          "progress": 50
        }
      },
      "expectations": {}
    },
    {
      "step": 4,
      "action": {
        "type": "send_ui_event",
        "data": {
          "type": "task_progress",
          "progress": 100
        }
      },
      "expectations": {}
    },
    {
      "step": 5,
      "action": {
        "type": "send_ui_event",
        "data": {
          "type": "task_completed",
          "result": "Embedding complete"
        }
      },
      "expectations": {}
    }
  ]
}
