{
  "name": "Semantic Search in Pre-made Workspace",
  "description": "Tests semantic search over rust-project workspace embeddings",
  "tags": ["context", "workspace", "embeddings", "semantic"],
  "setup": {
    "workspace": "rust-project",
    "terminal_size": [80, 24]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Find configuration loading code",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "configuration",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const result = await requestContext(",
          "    'src/config.rs',",
          "    'Need configuration loading code',",
          "    3",
          "  );",
          "  return 'Found config.rs with Config::load method';",
          "}"
        ]
      },
      "verify": {
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {
      "all_tasks_completed": true
    }
  }
}
