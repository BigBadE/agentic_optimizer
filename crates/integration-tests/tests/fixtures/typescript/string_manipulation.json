{
  "name": "TypeScript String Manipulation",
  "description": "Tests string methods and operations in TypeScript runtime",
  "tags": [
    "typescript",
    "strings"
  ],
  "setup": {
    "terminal_size": [
      80,
      24
    ]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Process text strings",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "text",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  const text = 'Hello World';",
          "  const upper = text.toUpperCase();",
          "  const words = text.split(' ');",
          "  const reversed = text.split('').reverse().join('');",
          "  return `Upper: ${upper}, Words: ${words.length}, Reversed: ${reversed}`;",
          "}"
        ]
      },
      "verify": {
        "ui": {
          "tasks_displayed": 1,
          "output_contains": [
            "HELLO WORLD",
            "Words: 2",
            "dlroW olleH"
          ]
        },
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {
      "all_tasks_completed": true
    }
  }
}
