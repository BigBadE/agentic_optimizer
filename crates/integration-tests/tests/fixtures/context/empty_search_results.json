{
  "name": "Empty Search Results",
  "description": "Tests context builder behavior when semantic search returns no results",
  "tags": ["context", "semantic-search", "edge-case", "empty-results"],
  "setup": {
    "files": {
      "readme.md": "# Project README\n\nThis is a test project."
    },
    "terminal_size": [120, 40]
  },
  "events": [
    {
      "type": "user_input",
      "data": {
        "text": "Find quantum entanglement implementation",
        "submit": true
      }
    },
    {
      "type": "llm_response",
      "trigger": {
        "pattern": "quantum entanglement",
        "match_type": "contains"
      },
      "response": {
        "typescript": [
          "async function agent_code(): Promise<string> {",
          "  return 'No quantum entanglement implementation found in this project';",
          "}"
        ]
      },
      "verify": {
        "execution": {}
      }
    }
  ],
  "final_verify": {
    "execution": {}
  }
}
